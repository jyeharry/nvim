{
  "Typescript React Function Component": {
    "prefix": "fc",
    "body": [
      "import { FC } from 'react'",
      "",
      "interface ${TM_FILENAME_BASE/(.*)/${1:/capitalize}/}Props {",
      "  $1",
      "}",
      "",
      "const ${TM_FILENAME_BASE/(.*)/${1:/capitalize}/}: FC<${TM_FILENAME_BASE/(.*)/${1:/capitalize}/}Props> = ({$2}) => {",
      "  return ($3)",
      "}",
      "",
      "export default ${TM_FILENAME_BASE/(.*)/${1:/capitalize}/}"
    ],
    "description": "Typescript React Function Component"
  },
  "NextJS Layout Component": {
    "prefix": "lc",
    "body": [
      "import { FC, ReactNode } from 'react'",
      "",
      "interface $1LayoutProps {",
      "  children: ReactNode",
      "}",
      "",
      "const $1Layout: FC<$1LayoutProps> = ({ children }) => {",
      "  return ($2)",
      "}",
      "",
      "export default $1Layout"
    ]
  }
}
